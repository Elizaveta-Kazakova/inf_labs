/usr/bin/less
/usr/bin/grep
/usr/bin/find
elizaveta@elizaveta-VirtualBox:~$ whereis  cat less grep find
cat: /usr/bin/cat /usr/share/man/man1/cat.1.gz
less: /usr/bin/less /usr/share/man/man1/less.1.gz
grep: /usr/bin/grep /usr/share/man/man1/grep.1.gz /usr/share/info/grep.info.gz
find: /usr/bin/find /usr/share/man/man1/find.1.gz /usr/share/info/find.info-1.gz /usr/share/info/find.info-2.gz /usr/share/info/find.info.gz
elizaveta@elizaveta-VirtualBox:~$ echo $PATH
/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin
